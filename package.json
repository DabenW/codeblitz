{
  "name": "@codeblitzjs/core",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "packages/*"
  ],
  "description": "codeblitzjs",
  "main": "index.js",
  "engines": {
    "opensumi": "2.23.0"
  },
  "scripts": {
    "prepare": "husky install",
    "type-check": "tsc --skipLibCheck --noEmit",
    "preinstall": "node scripts/preinstall && npm run prepare:env",
    "prepare:env": "yum --version && sudo yum -y install unzip gcc-c++ libX11-devel libxkbfile-devel libsecret-devel || echo 'no yum'",
    "init": "npm run clean && npm run generate && npm run build && npm run ext",
    "start": "npm run dev",
    "ext": "alex ext",
    "generate": "node scripts/generate",
    "editor": "INTEGRATION=editor npm run dev",
    "fs": "INTEGRATION=filesystem npm run dev",
    "build": "node scripts/build",
    "bundle": "node scripts/bundle",
    "create": "node scripts/create",
    "format": "prettier --write --parser typescript \"packages/**/*.ts?(x)\"",
    "dev": "webpack serve --config packages/toolkit/webpack/config.dev.js",
    "clean": "rimraf packages/*/lib",
    "test": "jest --runInBand",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "node scripts/release",
    "release:next": "node scripts/release --tag=next"
  },
  "author": "antgroup",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "repository": "git@github.com:opensumi/codeblitz.git",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.6",
    "chalk": "^4.1.0",
    "conventional-changelog-cli": "^2.1.1",
    "dotenv": "^8.2.0",
    "execa": "^4.1.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "husky": "^5.0.4",
    "inquirer": "^7.3.3",
