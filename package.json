{
  "name": "alex",
  "version": "0.5.1",
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "description": "Ant Codespaces for browser",
  "main": "index.js",
  "engines": {
    "kaitian": "1.33.0"
  },
  "scripts": {
    "preinstall": "node scripts/preinstall",
    "init": "npm run clean && npm run build && npm run ext",
    "start": "npm run dev",
    "ext": "alex ext",
    "inspect": "INTEGRATION=inspect npm run dev",
    "build": "node scripts/build",
    "bundle": "node scripts/bundle",
    "create": "node scripts/create",
    "format": "prettier --write --parser typescript \"packages/**/*.ts?(x)\"",
    "dev": "webpack serve --config packages/toolkit/webpack/config.dev.js",
    "clean": "rimraf packages/*/lib",
    "test": "jest --runInBand",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "author": "antgroup",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npm.alibaba-inc.com"
  },
  "repository": {
    "type": "git",
    "url": "git@code.alipay.com:cloud-ide/crew-dragon.git"
  },
  "devDependencies": {
    "@ali/ide-extension-installer": "^2.1.0",
    "@alipay/basement": "^3.5.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.6",
    "chalk": "^4.1.0",
    "conventional-changelog-cli": "^2.1.1",
    "dotenv": "^8.2.0",
    "execa": "^4.1.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "husky": "^4.3.0",
    "inquirer": "^7.3.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "minimist": "^1.2.5",
    "prettier": "^2.1.2",
    "signale": "^1.4.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "urllib": "^2.36.1"
  },
  "tnpm": {
    "mode": "yarn",
    "lockfile": "enable"
  },
  "cloudideExtensions": [
    {
      "publisher": "cloud-ide-ext",
      "name": "sarif-viewer",
      "version": "0.0.9"
    },
    {
      "publisher": "alex",
      "name": "css-language-features-worker",
      "version": "1.0.0-beta.1"
    },
    {
      "publisher": "alex",
      "name": "markdown-language-features-worker",
      "version": "1.0.0-beta.1"
    },
    {
      "publisher": "alex",
      "name": "html-language-features-worker",
      "version": "1.0.0-beta.1"
    },
    {
      "publisher": "alex",
      "name": "json-language-features-worker",
      "version": "1.0.0-beta.1"
    },
    {
      "publisher": "alex",
      "name": "typescript-language-features-worker",
      "version": "1.0.0-beta.2"
    },
    {
      "publisher": "cloud-ide",
      "name": "vscode-lsif",
      "version": "0.1.2"
    }
  ]
}
