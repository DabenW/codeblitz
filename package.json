    "lint-staged": "^10.5.1",
    "localforage": "^1.10.0",
    "minimist": "^1.2.5",
    "prettier": "^2.1.2",
    "signale": "^1.4.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.3",
    "urllib": "^2.36.1",
    "shelljs": "^0.8.4"
  },
