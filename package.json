{
  "name": "alex",
  "version": "2.0.21",
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "description": "Ant Codespaces for browser",
  "main": "index.js",
  "engines": {
    "kaitian": "2.19.10"
  },
  "scripts": {
    "type-check": "tsc --skipLibCheck --noEmit",
    "preinstall": "node scripts/preinstall && npm run prepare:env",
    "prepare:env": "yum --version && sudo yum -y install unzip gcc-c++ libX11-devel libxkbfile-devel libsecret-devel || echo 'no yum'",
    "init": "npm run clean && npm run generate && npm run build && npm run ext",
    "start": "npm run dev",
    "start:acr": "INTEGRATION=antcode-cr npm run start",
    "ext": "alex ext",
    "generate": "node scripts/generate",
    "editor": "INTEGRATION=editor npm run dev",
    "acr": "INTEGRATION=antcode-cr npm run dev",
    "build": "node scripts/build",
    "bundle": "node scripts/bundle",
    "create": "node scripts/create",
    "format": "prettier --write --parser typescript \"packages/**/*.ts?(x)\"",
    "dev": "webpack serve --config packages/toolkit/webpack/config.dev.js",
    "clean": "rimraf packages/*/lib",
    "test": "jest --runInBand",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "node scripts/release",
    "release:next": "node scripts/release --tag=next"
  },
  "author": "antgroup",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npm.alibaba-inc.com"
  },
  "repository": {
    "type": "git",
    "url": "git@code.alipay.com:cloud-ide/crew-dragon.git"
  },
  "devDependencies": {
    "@ali/ide-extension-installer": "^2.1.0",
    "@alipay/basement": "^3.5.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.6",
    "chalk": "^4.1.0",
    "conventional-changelog-cli": "^2.1.1",
