{
  "name": "worker.example",
  "version": "0.0.1",
  "publisher": "alex",
  "engines": {
    "kaitian": "*"
  },
  "activationEvents": [
    "*"
  ],
  "kaitianContributes": {
    "workerMain": "./worker.js",
    "browserMain": "./browser.js",
    "browserViews": {
      "left": {
        "type": "add",
        "view": [
          {
            "id": "Component",
            "icon": "extension"
          }
        ]
      }
    }
  },
  "contributes": {
    "jsonValidation": [
      {
        "fileMatch": "/package.json",
        "url": "./package.schema.json"
      }
    ],
    "yamlValidation": [
      {
        "fileMatch": "/my.yml",
        "url": "./package.schema.json"
      }
    ]
  }
}
